{
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/require-default-props": 0,
    "react/no-array-index-key": 0,
    "import/prefer-default-export": 0,
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
  }
}
